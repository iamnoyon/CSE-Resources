clc
clear all
close all
fs=1000;
t=0:1/fs:1;
L=100;
x=5*cos(2*pi*100*t)+randn(size(t));
q=hamming(L);
q1=hanning(L);
q2=kaiser(L,12.5);
q3=rectwin(L);
psd(x,L,fs,q);
hold on
psd(x,L,fs,q1);

psd(x,L,fs,q2);
psd(x,L,fs,q3);

plot(a1,'r')
hold on
plot(a2,'black')
hold on
plot(a3,'g')
legend('Hamming','hanning','kaiser','rectgular')